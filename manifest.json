{
	"manifest_version": 2,
  "short_name": "BookmarkTab",
  "name": "Bookmark Tab",
  "version": "1.0.0",
  "description": "Chrome Extension for bookmarking tabs made simple",
  "icons": {
      "16": "assets/logo16.png",
      "32": "assets/logo48.png",
      "128": "assets/logo128.png"
    },
  "browser_action": {
    "default_icon": "assets/logo128",
    "default_popup": "popup.html"
  },

  "content_security_policy": "script-src 'self' 'sha256-vvt4KWwuNr51XfE5m+hzeNEGhiOfZzG97ccfqGsPwvE='; object-src 'self'",
  "permissions" : [
    "storage",
    "tabs",
    "<all_urls>"
  ]
}
